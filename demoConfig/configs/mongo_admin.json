{
    "host": {
        "doc": "MongoDB admin host for creating new users",
        "default": "10.25.2.2",
        "env": "MONGO_ADMIN_HOST"
    },
    "port": {
        "doc": "MongoDB admin port for creating new users",
        "format": "int",
        "default": 27017,
        "env": "MONGO_ADMIN_PORT"
    },
    "userName": {
        "doc": "MongoDB admin username for creating new users",
        "default": "admin",
        "env": "MONGO_ADMIN_USERNAME"
    },
    "password": {
        "doc": "MongoDB admin password for creating new users",
        "default": "hxgphJsiFizLjxZ",
        "env": "MONGO_ADMIN_PASSWORD"
    },
    "database": {
        "doc": "MongoDB admin database name",
        "default": "admin",
        "env": "MONGO_ADMIN_DATABASE"
    },
    "authSource": {
        "doc": "MongoDB admin authentication source",
        "default": "admin",
        "env": "MONGO_ADMIN_AUTH_SOURCE"
    }
}

