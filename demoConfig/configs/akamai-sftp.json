        "env" : "AKAMAI_SFTP_HOST"
    },
    "user": {
        "doc" : "user id",
        "default" : "sshacs",
        "env" : "AKAMAI_SFTP_USER"
    },
    "privateKey": {
        "doc" : "password", 
        "default" : "/Users/rama.m/Downloads/jiocricket",
        "env" : "AKAMAI_SFTP_PRIVATE_KEY_PATH"
    },
    "path": {
        "doc" : "location of homepage json",
        "default":"/714164/ipl2020test",
        "env" : "AKAMAI_SFTP_PATH"
    },
    "url": {
        "doc" : "public url",
        "default" : "https://jiokbc.akamaized.net/ipl2020test",
